@model ATS.WEB.Data.Entities.TestResult

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>TestIntro</title>
</head>
<body>

<h4>TestResult</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="TestIntro">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="Id" />
            <div class="form-group">
                <label asp-for="AnswererId" class="control-label"></label>
                <select asp-for="AnswererId" class="form-control" asp-items="ViewBag.AnswererId"></select>
                <span asp-validation-for="AnswererId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="TopicId" class="control-label"></label>
                <select asp-for="TopicId" class="form-control" asp-items="ViewBag.TopicId"></select>
                <span asp-validation-for="TopicId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="PassDate" class="control-label"></label>
                <input asp-for="PassDate" class="form-control" />
                <span asp-validation-for="PassDate" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Score" class="control-label"></label>
                <input asp-for="Score" class="form-control" />
                <span asp-validation-for="Score" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

</body>
</html>
