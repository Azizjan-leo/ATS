@page
@model ATS.WEB.Areas.Admin.Pages.Students.IndexModel

@{
    ViewData["Title"] = "Контакты";
}

<h1>Контакты</h1>

<table class="table">
    <thead>
        <tr>
            <th>Имя</th>
            <th>Кафедра</th>
            <th>Почта</th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.Student) {
        <tr>
            <td>
                @item.User.Name
            </td>
            <td>
                @item.Cathedra.Name
            </td>
            <td>
                @item.User.Email
            </td>
        </tr>
}
    </tbody>
</table>
